{% extends 'base.html.twig' %}

{% block title %}Edit article{% endblock %}

{% block body %}
    <h4>Editer un article</h4>
    {{ form_start(form) }}
    {{ form_row(form.title) }}
    {{ form_row(form.content) }}
    {{ form_row(form.category) }}
    <button class="btn">{{ button_label|default('Save') }}</button>
    {{ form_end(form) }}
{% endblock %}
